model AuthorizedAgentDomains {
  id        String   @id @default(uuid())
  agentId   String
  domain    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  agent     Agents   @relation(fields: [agentId], references: [id])

  @@map("authorized_agent_domains")
  @@schema("ai")
}
